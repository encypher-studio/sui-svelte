{
    "name": "sui-svelte",
    "version": "0.0.11",
    "repository": {
        "type": "git",
        "url": "https://github.com/encypher-studio/sui-svelte"
    },
    "homepage": "https://encypherstudio.com",
    "keywords": [
        "svelte",
        "sui",
        "web3",
        "blockchain",
        "components",
        "sveltekit",
        "wallet"
    ],
    "scripts": {
        "dev": "vite dev",
        "build": "vite build && pnpm package",
        "preview": "vite preview",
        "package": "svelte-kit sync && svelte-package && publint",
        "prepublishOnly": "pnpm  package",
        "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
        "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
        "test": "vitest",
        "lint": "prettier --check . && eslint .",
        "format": "prettier --write ."
    },
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "svelte": "./dist/index.js"
        },
        "./ConnectButton": {
            "types": "./dist/ConnectButton/index.d.ts",
            "svelte": "./dist/ConnectButton/index.js"
        },
        "./ConnectModal": {
            "types": "./dist/ConnectModal/index.d.ts",
            "svelte": "./dist/ConnectModal/index.js"
        },
        "./SuiModule": {
            "types": "./dist/SuiModule/index.d.ts",
            "svelte": "./dist/SuiModule/index.js"
        }
    },
    "files": [
        "dist",
        "!dist/**/*.test.*",
        "!dist/**/*.spec.*"
    ],
    "peerDependencies": {
        "svelte": ">=5.0.0 || <6.0.0"
    },
    "devDependencies": {
        "@sveltejs/adapter-auto": "^4.0.0",
        "@sveltejs/kit": "^2.16.1",
        "@sveltejs/package": "^2.3.8",
        "@sveltejs/vite-plugin-svelte": "^5.0.3",
        "@types/eslint": "9.6.1",
        "@typescript-eslint/eslint-plugin": "^8.21.0",
        "@typescript-eslint/parser": "^8.21.0",
        "eslint": "^9.18.0",
        "eslint-config-prettier": "^10.0.1",
        "eslint-plugin-svelte": "^2.46.1",
        "prettier": "^3.4.2",
        "prettier-plugin-svelte": "^3.3.3",
        "publint": "^0.3.2",
        "svelte": "^5.19.1",
        "svelte-check": "^4.1.4",
        "tslib": "^2.8.1",
        "typescript": "^5.7.3",
        "vite": "^6.0.11",
        "vitest": "^3.0.3"
    },
    "svelte": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "type": "module",
    "dependencies": {
        "@mysten/sui": "^1.20.0",
        "@mysten/wallet-standard": "^0.13.23",
        "@suiet/wallet-sdk": "^0.3.2",
        "@wallet-standard/base": "^1.1.0"
    },
    "packageManager": "pnpm@9.15.4+sha512.b2dc20e2fc72b3e18848459b37359a32064663e5627a51e4c74b2c29dd8e8e0491483c3abb40789cfd578bf362fb6ba8261b05f0387d76792ed6e23ea3b1b6a0"
}
